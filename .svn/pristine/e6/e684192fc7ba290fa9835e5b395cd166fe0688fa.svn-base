
public class BinomialNode {
	public BinomialNode parent;
	public BinomialNode rightSib;
	public BinomialNode leftChild;
	public int priority;
	public int value;
	public int degree;
	
	BinomialNode(BinomialNode parent, BinomialNode leftChild,BinomialNode rightSib, int priority, int value ){
		this.parent = parent;
		this.leftChild= leftChild;
		this.rightSib = rightSib;
		this.priority = priority;
		this.value  = value;
		degree = 0;
	}

	
}
